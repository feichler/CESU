{% for message in error %}
    <div class="system-message system-message-error system-message-dismissable">
        <button type="button" class="close" data-dismiss="alert">
            <span aria-hidden="true">
                &times;
            </span>
            <span class="sr-only">
                Close
            </span>
        </button>
        {{ message|msg }}
    </div>
{% endfor %}